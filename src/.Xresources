*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! Black
*.color0:   #282a2e
*.color8:   #373b41

! Red
*.color1:   #a54242
*.color9:   #cc6666

! Green
*.color2:   #8c9440
*.color10:  #b5bd68

! Yellow
*.color3:   #de935f
*.color11:  #f0c674

! Blue
*.color4:   #5f819d
*.color12:  #81a2be

! Magenta
*.color5:   #85678f
*.color13:  #b294bb

! Cyan
*.color6:   #5e8d87
*.color14:  #8abeb7

! White
*.color7:   #707880
*.color15:  #c5c8c6

URxvt.font:             xft:Noto Sans Mono:style=SemiCondensed, xft:DejaVu Sans Mono:style=Regular, xft:Inconsolata for Powerline, xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true,xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true
URxvt.boldFont:         xft:Noto Sans Mono:style=SemiCondensed Bold, xft:DejaVu Sans Mono:style=Bold, xft:Inconsolata for Powerline, xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true,xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true
URxvt.italicFont:       xft:Noto Sans Mono:italic:style=SemiCondensed, xft:DejaVu Sans Mono:style=Regular, xft:Inconsolata for Powerline, xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true,xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true
URxvt.boldItalicFont:   xft:Noto Sans Mono:italic:style=SemiCondensed Bold, xft:DejaVu Sans Mono:style=Bold, xft:Inconsolata for Powerline, xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true,xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true

URxvt.scrollBar:        false
URxvt.urgentOnBell:     true
URxvt.loginShell:       false
! True transparency
!URxvt.depth:            32
!URxvt.background:       [90]#1d1f21
! Native transparency
!URxvt.inheritPixmap:    true
!URxvt.transparent:      true
!URxvt.shading:          20

Xcursor.theme: Neutral
